<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.aaa.mapper.UserstowmodileMapper" >
  <resultMap id="BaseResultMap" type="com.aaa.bean.Userstowmodile" >
    <result column="mTowId" property="mtowid" jdbcType="INTEGER" />
    <result column="uid" property="uid" jdbcType="INTEGER" />
  </resultMap>
  <!-- List<Users> getTwoUsers(Integer mTowId); -->
  <select id="getTwoUsers" parameterType="integer" resultType="com.aaa.bean.Userstowmodile">
  	SELECT * FROM userstowmodile ut WHERE ut.mTowId = #{mTowId}
  </select>
  <delete id="delUserstowmodile" parameterType="com.aaa.bean.Userstowmodile">
  	DELETE FROM userstowmodile WHERE uid = #{uid} AND mTowId = #{mtowid};
  </delete>
  <insert id="insert" parameterType="com.aaa.bean.Userstowmodile" >
    insert into userstowmodile (mTowId, uid)
    values (#{mtowid,jdbcType=INTEGER}, #{uid,jdbcType=INTEGER})
  </insert>
  <insert id="insertSelective" parameterType="com.aaa.bean.Userstowmodile" >
    insert into userstowmodile
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="mtowid != null" >
        mTowId,
      </if>
      <if test="uid != null" >
        uid,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="mtowid != null" >
        #{mtowid,jdbcType=INTEGER},
      </if>
      <if test="uid != null" >
        #{uid,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
</mapper>